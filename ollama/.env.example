# Model storage configuration
MODELS_PATH=./downloaded_models

# API key configuration
# This is for local development only - generate a secure key for production
# To generate a secure key: openssl rand -hex 32
OLLAMA_API_KEY=your_secure_key_here

# Ollama configuration
OLLAMA_CONTEXT_LENGTH=128000
OLLAMA_KEEP_ALIVE=3h

# WebUI configuration
WEBUI_AUTH=false
